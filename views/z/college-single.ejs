  <!-- all tabs -->
  <div class="all_tabs container ">
    <div style="margin: 6rem auto;">
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item" role="presentation">
          <button class="nav-link active" id="about-tab" data-bs-toggle="tab" data-bs-target="#about" type="button"
            role="tab" aria-controls="about" aria-selected="true">About</button>
        </li>
        <li class="nav-item" role="presentation">
          <button class="nav-link" id="gallery-tab" data-bs-toggle="tab" data-bs-target="#gallery" type="button"
            role="tab" aria-controls="gallery" aria-selected="false">Gallery</button>
        </li>
        <li class="nav-item" role="presentation">
          <button class="nav-link" id="courfee-tab" data-bs-toggle="tab" data-bs-target="#courfee" type="button"
            role="tab" aria-controls="courfee" aria-selected="false">Courses & Fees</button>
        </li>
        <li class="nav-item" role="presentation">
          <button class="nav-link" id="test-tab" data-bs-toggle="tab" data-bs-target="#test" type="button" role="tab"
            aria-controls="test" aria-selected="false">Testimonials</button>
        </li>
        <li class="nav-item" role="presentation">
          <button class="nav-link" id="alumni-tab" data-bs-toggle="tab" data-bs-target="#alumni" type="button"
            role="tab" aria-controls="alumni" aria-selected="false">Alumni</button>
        </li>
        <!-- <li class="nav-item" role="presentation">
          <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#contact" type="button"
            role="tab" aria-controls="contact" aria-selected="false">Contact</button>
        </li>
        <li class="nav-item" role="presentation">
          <button class="nav-link" id="brochure-tab" data-bs-toggle="tab" data-bs-target="#brochure" type="button"
            role="tab" aria-controls="brochure" aria-selected="false">Brochure</button>
        </li> -->
      </ul>
      <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="about" role="tabpanel" aria-labelledby="about-tab">
          <h3>Indian Institute of Technology, Chennai </h3>
          <p>
            <%= college.description %>
          </p>
        </div>
        <div class="tab-pane fade" id="gallery" role="tabpanel" aria-labelledby="gallery-tab">
          <div class="row gy-4 my-4 my-md-0">
            <% gallery.forEach((image, index)=> { %>
              <div class="col-12 col-md-<%= index % 2 === 0 ? '5' : '6' %>">
                <div style="height: 168px">
                  <img src="<%= process.env.APP_URL %><%= image.image %>" class="card-img-top w-100 h-100"
                    alt="Gallery image <%= index + 1 %>" />
                </div>
              </div>
              <% }); %>
          </div>
        </div>
        <div class="tab-pane fade" id="courfee" role="tabpanel" aria-labelledby="courfee-tab">
          <div class="text-center py-5 p-2 p-md-5" id="courses_fees">
            <h1 class="fw-bold">Other Course Specialisation in same college</h1>
            <h5>
              Course Specialisation | <%= college.name %> [<%= college.shortName %>]-
                  <%= college.city.cityName %>, <%= college.state.stateName %>
            </h5>
            <div class="table-responsive mt-4">
              <table class="table table-hover">
                <thead>
                  <tr class="text-center">
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Course Name
                    </th>
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Course Specialisation
                    </th>
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Course Fee
                    </th>
                  </tr>
                </thead>
                <% courses.forEach(function(course) { %>
                  <tbody class="border-1">
                    <tr>
                      <td class="border-0">3<%= course.parentCourse %>
                      </td>
                      <td class="border-0">3<%= course.subCourse %>
                      </td>
                      <td class="border-0">Rs <%= course.fee %>
                      </td>
                    </tr>
                  </tbody>
                  <% }); %>
              </table>
            </div>
          </div>
        </div>
        <div class="tab-pane fade" id="test" role="tabpanel" aria-labelledby="courfee-tab">
          <div class="row">
            <h1 class="fw-bold text-center">
              Testimonials
            </h1>
            <div class="col-md-12" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="100">
              <div class="carousel-testimony">
                <% testimonials.forEach(function(testimonial) { %>
                  <div class="item">
                    <div class="testimony-wrap">
                      <div class="text">
                        <div class="d-flex align-items-center mb-4">
                          <div class="user-img"
                            style="background-image: url('<%=process.env.APP_URL%><%= testimonial.profileImage %>')">
                            <div class="icon d-flex align-items-center justify-content-center"><span
                                class="fa fa-quote-left"></span></div>
                          </div>
                          <div class="ps-3 tx">
                            <p class="name">
                              <%= testimonial.name %>
                            </p>
                            <span class="position">
                              <%= testimonial.designation %>
                            </span>
                          </div>
                        </div>
                        <p class="mb-4 msg">
                          <%= testimonial.message %>
                        </p>
                      </div>
                    </div>
                  </div>
                  <% }); %>
              </div>
            </div>
          </div>
        </div>
        <div class="tab-pane fade" id="alumni" role="tabpanel" aria-labelledby="alumni-tab">
          <div class="py-5 p-2 p-md-5" id="alumnus">
            <h3 class="fw-bold">Alumnus</h3>
            <div class="table-responsive">
              <table class="table table-hover table-striped">
                <thead>
                  <tr class="text-center">
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Full Name
                    </th>
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Passing Year
                    </th>
                    <th scope="col" class="border border-1 text-white" style="background-color: #1eb2a6">
                      Placement
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <% alumni.forEach(function(alumnus) { %>
                    <tr class="text-center">
                      <td class="border border-1">
                        <%= alumnus.name %>
                      </td>
                      <td class="border border-1">
                        <%= alumnus.batch %>
                      </td>
                      <td class="border border-1">
                        <%= alumnus.package %>
                      </td>
                    </tr>
                    <% }); %>
                </tbody>
              </table>
            </div>
          </div>
          <!-- <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">contact</div>
          <div class="tab-pane fade" id="brochure" role="tabpanel" aria-labelledby="brochure-tab">brochure</div> -->
        </div>
      </div>
    </div>

    <!-- check elligibility -->

