<%- include('head') %>
  <style>
    .carousel,
    .carousel-inner,
    .carousel-item,
    img {
      height: 100%;
    }

    .carousel-item img {
      object-fit: cover;
    }

    .zind {
      z-index: 1;
    }

    .clg_img {
      width: 200px;
      height: 200px;
    }

    .clg_img img {
      height: 100%;
      width: 100%;
      object-fit: fill;
    }

    .btnty {
      background: #1eb2a6;
    }

    .btn:hover {
      background: #1eb2a6;
    }

    .btnbo {
      border: 1px solid #1eb2a6;
    }

    .nav-link.active {
      border-bottom: 2px solid #1eb2a6 !important;
      color: #1eb2a6 !important;

    }

    .nav-link {
      color: rgba(33, 37, 41, .75);
      border: 0px !important;
    }

    .nav-link:hover {
      border: 0px;
      color: black;
    }

    .courses {
      background: linear-gradient(360deg, #fffcfc 14%, transparent), url('<%= process.env.APP_URL %>nd/images/clg_1.jpg');
      background-size: cover;
    }

    .hov {
      opacity: 0;
    }

    .galp:hover .hov {
      opacity: 1;
      transition: all .3s ease-in;
    }
  </style>
